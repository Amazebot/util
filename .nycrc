{
  "check-coverage": true,
  "per-file": true,
  "lines": 90,
  "statements": 90,
  "functions": 80,
  "branches": 80,
  "extension": [
    ".ts"
  ],
  "require": [
    "ts-node/register"
  ],
  "exclude": [
    "**/src/index.ts",
    "**/*.d.ts",
    "**/*.spec.ts"
  ],
  "reporter": [
    "lcovonly",
    "text"
  ]
}